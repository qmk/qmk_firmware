        "rotary": [
            {"pin_a": "B4", "pin_b": "B2"},
            {"pin_a": "B5", "pin_b": "B6"}
        ]
    },
    "ws2812": {
        "pin": "D3"
    },
    "processor": "atmega32u4",
    "bootloader": "caterina",
    "layouts": {
