{
    "keyboard_name": "latinpad",
    "manufacturer": "latincompass",
    "url": "https://github.com/latincompass/2-RotaryEncoder-pad",
    "maintainer": "latincompass",
    "usb": {
        "vid": "0x6C63",
        "pid": "0x6C70",
        "device_version": "0.0.1"
    },
    "rgb_matrix": {
        "driver": "WS2812"
    },
    "matrix_pins": {
        "cols": ["D4", "C6", "D7", "E6"],
        "rows": ["F4", "F5", "F6", "F7", "B1"]
    },
    "diode_direction": "ROW2COL",
