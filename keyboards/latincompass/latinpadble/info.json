{
    "keyboard_name": "latinpadble",
    "url": "https://github.com/latincompass/2-RotaryEncoder-pad",
    "maintainer": "haierwangwei2005",
    "layouts": {
        "LAYOUT_pad": {
            "layout": [
                {"label":"Num Lock", "x":0, "y":0, "w":0.8, "h":0.8},

                {"label":"7", "x":0, "y":1},
                {"label":"8", "x":1, "y":1},
                {"label":"9", "x":2, "y":1},
                {"label":"+", "x":3, "y":1},

                {"label":"4", "x":0, "y":2},
                {"label":"5", "x":1, "y":2},
                {"label":"6", "x":2, "y":2},
                {"label":"+", "x":3, "y":2},

                {"label":"1", "x":0, "y":3},
                {"label":"2", "x":1, "y":3},
                {"label":"3", "x":2, "y":3},
                {"label":"Enter", "x":3, "y":3},

                {"label":"0", "x":0, "y":4},
                {"label":"0", "x":1, "y":4},
                {"label":".", "x":2, "y":4},
                {"label":"Enter", "x":3, "y":4}
            ]
        }
    }
