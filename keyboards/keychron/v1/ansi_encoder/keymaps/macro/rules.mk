# Keychron V1 ANSI Knob - Command Macro Build Rules

# Core features
VIA_ENABLE = yes            # Enable VIA support for remapping
ENCODER_ENABLE = yes        # Enable rotary encoder support
MOUSEKEY_ENABLE = yes       # Enable mouse keys
EXTRAKEY_ENABLE = yes       # Enable media keys
NKRO_ENABLE = yes          # Enable N-Key Rollover

# Optimization
LTO_ENABLE = yes           # Link Time Optimization (reduce firmware size)
