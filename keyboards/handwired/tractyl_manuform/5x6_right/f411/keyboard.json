        "cols": ["A15", "B3", "B4", "B5", "B6", "B7"],
        "rows": ["B12", "B13", "B14", "B15", "A8", "A10"]
    },
    "diode_direction": "COL2ROW",
    "eeprom": {
        "driver": "spi"
    },
    "rgblight": {
