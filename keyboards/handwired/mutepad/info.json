{
    "keyboard_name": "MutePad",
    "manufacturer": "JoshwJB",
    "url": "https://github.com/JoshwJB/MutePad",
    "maintainer": "JoshwJB",
    "usb": {
        "vid": "0xFEED",
        "pid": "0x0000",
        "device_version": "0.0.1",
        "force_nkro": true
    },
    "matrix_pins": {
        "cols": ["B1", "B3", "B2", "B6"],
        "rows": ["F6"]
    },
    "diode_direction": "COL2ROW",
    "encoder": {
        "rotary": [
            {"pin_a": "F4", "pin_b": "F5", "resolution": 2}
        ]
    },
    "qmk": {
        "tap_keycode_delay": 10
    },
    "processor": "atmega32u4",
    "bootloader": "caterina",
    "layouts": {
        "LAYOUT": {
            "layout": [
                {"matrix": [0, 0], "x": 0, "y": 0},
                {"matrix": [0, 1], "x": 1, "y": 0},
                {"matrix": [0, 2], "x": 2, "y": 0},
                {"matrix": [0, 3], "x": 3, "y": 0}
            ]
        }
    }
}
