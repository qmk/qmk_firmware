{
    "keyboard_name": "2x5keypad",
    "manufacturer": "Jonathan Cameron",
    "maintainer": "jmcameron",
    "usb": {
        "vid": "0xFEED",
        "pid": "0x2020",
        "device_version": "0.0.1"
    },
    "tapping": {
        "term": 250
    },
    "features": {
        "bootmagic": true,
