          "alternating": true,
          "breathing": true,
          "christmas": true,
          "knight": true,
          "rainbow_mood": true,
          "rainbow_swirl": true,
          "rgb_test": true,
          "snake": true,
          "static_gradient": true,
          "twinkle": true
      },
      "brightness_steps": 10,
      "default": {
          "animation": "rainbow_swirl"
      },
      "layers": {
          "enabled": true,
          "override_rgb": false
      },
      "led_count": 1,
      "saturation_steps": 8
  },
  "url": "",
