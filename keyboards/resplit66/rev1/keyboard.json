{
    "manufacturer": "David Girault",
    "keyboard_name": "ReSplit66",
    "board": "RESPLIT66_V1",
    "processor": "STM32L443",
    "url": "https://gitlab.com/dgirault/resplit66",
    "maintainer": "dfgweb",
    "bootloader": "stm32-dfu",
    "diode_direction": "COL2ROW",
    "build": {
        "lto": false
    },
    "features": {
        "bootmagic": false,
        "console": true,
        "extrakey": true,
        "mousekey": true,
        "rgb_matrix": true,
    },
    "eeprom": {
        "driver": "wear_leveling",
        "wear_leveling": {
            "driver": "embedded_flash",
            "backing_size": 4096,
        }
    },
    "matrix_pins": {
        "cols": ["A0", "A1", "A2", "A3", "A4", "A5", "A6"],
        "rows": ["B0", "B1", "B2", "B4", "B5"]
    },
    "usb": {
        "device_version": "1.0.0",
        "vid": "0x1D50",
        "pid": "0x6601",
        "shared_endpoint": {
            "keyboard": true
        }
    },
    "split": {
        "enabled": true,
        "handedness": {
            "pin": "H1",
        },
        "usb_detect" : {
            "timeout": 500
        },
        "matrix_pins": {
            "right": {
                "cols": ["A6", "A5", "A4", "A3", "A2", "A1", "A0"],
            }
        },
        "transport": {
            "protocol": "custom",
            "sync": {
                "indicators": true,
                "modifiers": true,
                "layer_state": true,
                "activity": true,
            },
        },
    },
    "rgb_matrix": {
        "driver": "custom",
        "animations": {
            "alphas_mods": true,
            "gradient_up_down": true,
            "gradient_left_right": true,
            "breathing": true,
            "band_sat": true,
            "band_val": true,
            "band_pinwheel_sat": true,
            "band_pinwheel_val": true,
            "band_spiral_sat": true,
            "band_spiral_val": true,
            "cycle_all": true,
            "cycle_left_right": true,
            "cycle_up_down": true,
            "rainbow_moving_chevron": true,
            "cycle_out_in": true,
            "cycle_out_in_dual": true,
            "cycle_pinwheel": true,
            "cycle_spiral": true,
            "dual_beacon": true,
            "rainbow_beacon": true,
            "rainbow_pinwheels": true,
            "raindrops": true,
            "jellybean_raindrops": true,
            "hue_breathing": true,
            "hue_pendulum": true,
            "hue_wave": true,
            "pixel_fractal": true,
            "pixel_flow": true,
            "pixel_rain": true,
            "typing_heatmap": true,
            "digital_rain": true,
            "solid_reactive_simple": true,
            "solid_reactive": true,
            "solid_reactive_wide": true,
            "solid_reactive_multiwide": true,
            "solid_reactive_cross": true,
            "solid_reactive_multicross": true,
            "solid_reactive_nexus": true,
            "solid_reactive_multinexus": true,
            "splash": true,
            "multisplash": true,
            "solid_splash": true,
            "solid_multisplash": true
