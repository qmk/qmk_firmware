{
    "manufacturer": "David Girault",
    "keyboard_name": "ReSplit66",
    "board": "RESPLIT66_V1",
    "processor": "STM32L443",
    "url": "https://gitlab.com/dgirault/resplit66",
    "maintainer": "dfgweb",
    "bootloader": "stm32-dfu",
    "diode_direction": "COL2ROW",
    "build": {
        "lto": false
    },
    "features": {
        "bootmagic": false,
        "console": true,
        "extrakey": true,
        "mousekey": true,
        "rgb_matrix": false,
    },
    "eeprom": {
        "driver": "wear_leveling",
        "wear_leveling": {
            "driver": "embedded_flash",
            "backing_size": 4096,
        }
    },
    "matrix_pins": {
        "cols": ["A0", "A1", "A2", "A3", "A4", "A5", "A6"],
        "rows": ["B0", "B1", "B2", "B4", "B5"]
    },
    "usb": {
        "device_version": "1.0.0",
        "vid": "0x1D50",
        "pid": "0x6601",
        "shared_endpoint": {
            "keyboard": true
        }
    },
    "split": {
        "enabled": true,
        "handedness": {
            "pin": "H1",
        },
        "usb_detect" : {
            "timeout": 500
        },
        "matrix_pins": {
            "right": {
                "cols": ["A6", "A5", "A4", "A3", "A2", "A1", "A0"],
            }
        },
        "transport": {
            "protocol": "custom",
            "sync": {
                "indicators": true,
                "modifiers": true,
                "layer_state": true,
                "activity": true,
            },
        },
    },
    "layouts": {
        "LAYOUT": {
            "layout": [
                {"matrix": [0, 0], "hand": "L", "x": 0, "y": 0},//y+0.8
                {"matrix": [0, 1], "hand": "L", "x": 1, "y": 0},//y+0.8
                {"matrix": [0, 2], "hand": "L", "x": 2, "y": 0},//y+0.3
                {"matrix": [0, 3], "hand": "L", "x": 3, "y": 0},
                {"matrix": [0, 4], "hand": "L", "x": 4, "y": 0},//y+0.5
                {"matrix": [0, 5], "hand": "L", "x": 5, "y": 0},//y+0.5
                {"matrix": [0, 6], "hand": "L", "x": 6, "y": 0},//y+0.5
                {"matrix": [5, 0], "hand": "R", "x": 7, "y": 0},//y+0.5
                {"matrix": [5, 1], "hand": "R", "x": 8, "y": 0},//y+0.5
                {"matrix": [5, 2], "hand": "R", "x": 9, "y": 0},//y+0.5
                {"matrix": [5, 3], "hand": "R", "x": 10, "y": 0},
                {"matrix": [5, 4], "hand": "R", "x": 11, "y": 0},//y+0.3
                {"matrix": [5, 5], "hand": "R", "x": 12, "y": 0},//y+0.8
                {"matrix": [5, 6], "hand": "R", "x": 13, "y": 0},//y+0.8

                {"matrix": [1, 0], "hand": "L", "x": 0, "y": 1},
                {"matrix": [1, 1], "hand": "L", "x": 1, "y": 1},
                {"matrix": [1, 2], "hand": "L", "x": 2, "y": 1},
                {"matrix": [1, 3], "hand": "L", "x": 3, "y": 1},
                {"matrix": [1, 4], "hand": "L", "x": 4, "y": 1},
                {"matrix": [1, 5], "hand": "L", "x": 5, "y": 1},
                {"matrix": [1, 6], "hand": "L", "x": 6, "y": 1, "h": 1.5},
                {"matrix": [6, 0], "hand": "R", "x": 7, "y": 1, "h": 1.5},
                {"matrix": [6, 1], "hand": "R", "x": 8, "y": 1},
                {"matrix": [6, 2], "hand": "R", "x": 9, "y": 1},
                {"matrix": [6, 3], "hand": "R", "x": 10, "y": 1},
                {"matrix": [6, 4], "hand": "R", "x": 11, "y": 1},
                {"matrix": [6, 5], "hand": "R", "x": 12, "y": 1},
                {"matrix": [6, 6], "hand": "R", "x": 13, "y": 1},

                {"matrix": [2, 0], "hand": "L", "x": 0, "y": 2},
                {"matrix": [2, 1], "hand": "L", "x": 1, "y": 2},
                {"matrix": [2, 2], "hand": "L", "x": 2, "y": 2},
                {"matrix": [2, 3], "hand": "L", "x": 3, "y": 2},
                {"matrix": [2, 4], "hand": "L", "x": 4, "y": 2},
                {"matrix": [2, 5], "hand": "L", "x": 5, "y": 2},
                {"matrix": [7, 1], "hand": "R", "x": 8, "y": 2},
                {"matrix": [7, 2], "hand": "R", "x": 9, "y": 2},
                {"matrix": [7, 3], "hand": "R", "x": 10, "y": 2},
                {"matrix": [7, 4], "hand": "R", "x": 11, "y": 2},
                {"matrix": [7, 5], "hand": "R", "x": 12, "y": 2},
                {"matrix": [7, 6], "hand": "R", "x": 13, "y": 2},

                {"matrix": [3, 0], "hand": "L", "x": 0, "y": 3},
                {"matrix": [3, 1], "hand": "L", "x": 1, "y": 3},
                {"matrix": [3, 2], "hand": "L", "x": 2, "y": 3},
                {"matrix": [3, 3], "hand": "L", "x": 3, "y": 3},
                {"matrix": [3, 4], "hand": "L", "x": 4, "y": 3},
                {"matrix": [3, 5], "hand": "L", "x": 5, "y": 3},
                {"matrix": [3, 6], "hand": "L", "x": 6, "y": 2.5, "h": 1.5},
                {"matrix": [8, 0], "hand": "R", "x": 7, "y": 2.5, "h": 1.5},
                {"matrix": [8, 1], "hand": "R", "x": 8, "y": 3},
                {"matrix": [8, 2], "hand": "R", "x": 9, "y": 3},
                {"matrix": [8, 3], "hand": "R", "x": 10, "y": 3},
                {"matrix": [8, 4], "hand": "R", "x": 11, "y": 3},
                {"matrix": [8, 5], "hand": "R", "x": 12, "y": 3},
                {"matrix": [8, 6], "hand": "R", "x": 13, "y": 3},

                {"matrix": [4, 0], "hand": "L", "x": 0, "y": 4},
                {"matrix": [4, 1], "hand": "L", "x": 1, "y": 4},
                {"matrix": [4, 2], "hand": "L", "x": 2, "y": 4},
                {"matrix": [4, 3], "hand": "L", "x": 3, "y": 4},
                {"matrix": [4, 4], "hand": "L", "x": 4, "y": 4, "w": 1.5},
                {"matrix": [4, 5], "hand": "L", "x": 5.5, "y": 4, "w": 1.5},
                {"matrix": [9, 1], "hand": "R", "x": 7, "y": 4, "w": 1.5},
                {"matrix": [9, 2], "hand": "R", "x": 8.5, "y": 4, "w": 1.5},
                {"matrix": [9, 3], "hand": "R", "x": 10, "y": 4},
                {"matrix": [9, 4], "hand": "R", "x": 11, "y": 4},
                {"matrix": [9, 5], "hand": "R", "x": 12, "y": 4},
                {"matrix": [9, 6], "hand": "R", "x": 13, "y": 4},
            ]
        }
    }
}
