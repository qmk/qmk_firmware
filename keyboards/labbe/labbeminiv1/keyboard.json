{
    "keyboard_name": "Labbe Mini V1",
    "manufacturer": "Labbe",
    "maintainer": "qmk",
    "usb": {
        "vid": "0xABBE",
        "pid": "0x4C4D",
        "device_version": "0.0.1"
    },
    "features": {
        "bootmagic": true,
        "extrakey": false,
        "mousekey": false,
        "nkro": false
    },
    "qmk": {
        "locking": {
            "enabled": true,
            "resync": true
        }
    },
    "matrix_pins": {
        "cols": ["F0", "F1", "F4"],
        "rows": ["F5", "F6"]
    },
    "diode_direction": "COL2ROW",
    "processor": "atmega32u4",
    "bootloader": "atmel-dfu",
    "rgb_matrix": {
        "driver": "ws2812",
        "layout": [
            {"matrix": [0, 1], "x": 112, "y": 0, "flags": 4},
            {"matrix": [1, 0], "x": 0, "y": 64, "flags": 4},
            {"matrix": [1, 1], "x": 112, "y": 64, "flags": 4},
            {"matrix": [1, 2], "x": 224, "y": 64, "flags": 4}
        ]
    },
    "layouts": {
        "LAYOUT_wasd": {
            "layout": [
                {"matrix": [0, 1], "x": 1, "y": 0},

                {"matrix": [1, 0], "x": 0, "y": 1},
                {"matrix": [1, 1], "x": 1, "y": 1},
                {"matrix": [1, 2], "x": 2, "y": 1}
            ]
        }
    }
}
