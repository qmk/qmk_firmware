{
    "keyboard_name": "Charybdis Mini (3x6) STeMCell",
    "usb": {
        "device_version": "2.0.0",
        "shared_endpoint": {
            "keyboard": true
        }
    },
    "features": {
        "bootmagic": true,
        "mousekey": true,
        "extrakey": true,
        "rgb_matrix": true,
        "pointing_device": true
    },
    "rgb_matrix": {
        "driver": "ws2812"
    },
    "ws2812": {
        "pin": "A2",
        "driver": "pwm"
    },
    "build": {
        "debounce_type": "asym_eager_defer_pk"
    },
    "matrix_pins": {
        "cols": ["B1", "B2", "A4", "B4", "B5", "B8"],
        "rows": ["B0", "B3", "A15", "B9"]
    },
    "diode_direction": "ROW2COL",
    "split": {
        "enabled": true,
        "serial": {
            "pin": "A3"
        }
    },
    "development_board": "stemcell"
}
