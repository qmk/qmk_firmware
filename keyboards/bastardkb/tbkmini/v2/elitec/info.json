{
    "keyboard_name": "TBK Mini Elite-C",
    "usb": {
        "device_version": "2.0.0"
    },
    "features": {
        "bootmagic": true,
        "mousekey": true,
        "extrakey": true,
        "rgb_matrix": true
    },
    "ws2812": {
        "pin": "D3"
    },
    "rgb_matrix": {
        "driver": "ws2812"
    },
    "matrix_pins": {
        "cols": ["F6", "F5", "B6", "D7", "E6", "B4"],
        "rows": ["F7", "C6", "D4", "B5"]
    },
    "diode_direction": "ROW2COL",
    "split": {
        "enabled": true,
        "soft_serial_pin": "D2"
    },
    "processor": "atmega32u4",
    "bootloader": "atmel-dfu"
}
