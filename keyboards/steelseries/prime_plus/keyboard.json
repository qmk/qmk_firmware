        "device_version": "1.0.0",
        "pid": "0x0000",
        "vid": "0xFEED"
    },
    "layouts": {
        "LAYOUT": {
            "layout": [
                {"matrix": [0, 0], "x": 0, "y": 0},
                {"matrix": [0, 1], "x": 1, "y": 0},
                {"matrix": [0, 2], "x": 2, "y": 0},
                {"matrix": [0, 3], "x": 0, "y": 1},
                {"matrix": [0, 4], "x": 0, "y": 2},
                {"matrix": [0, 5], "x": 2, "y": 2}
            ]
        }
    }
}
