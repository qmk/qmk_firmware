{
    "processor": "atmega32u4",
    "bootloader": "atmel-dfu",

    "matrix_pins": {
        "rows": ["C6", "D7", "E6", "B4", "B5"],
        "cols": ["F6", "F7", "B1", "B3", "B2", "B6"]
     },    
    "diode_direction": "COL2ROW",

    "usb": {
      "vid": "0x4D4D",
      "pid": "0x4852",
      "device_version": "0.0.1"
    },
    "encoder": {
        "rotary": [
            {"pin_a": "F5", "pin_b": "F4"}
        ]
    },

    "features": {
        "encoder": true,
        "extrakey": true,
        "rgblight": true
    },
    "split": {
        "soft_serial_pin": "D2",
        "encoder": {
            "right": {
                "rotary": [
                    {"pin_a": "F4", "pin_b": "F5"}
                ]
            }
        }
    },

    "rgblight": {
        "led_count": 5,
        "pin": "D3",
        "split": true,
        "hue_steps": 8,
        "saturation_steps": 8,
        "brightness_steps": 8
    }
}
