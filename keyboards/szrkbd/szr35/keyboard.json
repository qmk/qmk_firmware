    "features": {
        "bootmagic": true,
        "extrakey": true,
        "mousekey": true,
        "nkro": true,
        "rgb_matrix": true,
    },
    "dynamic_keymap": {
        "layer_count": 9
    },
    "eeprom": {
        "wear_leveling": {
            "backing_size": 4096
        }
    },
    "matrix_pins": {
        "direct": [
            ["B4", "B0", "B1", "B12", "B13"],
            ["B5", "C5", "B2", "B14", "B15"],
            ["B3", "C4", "B10", "C9", "C11"],
            ["NO_PIN", "NO_PIN", "A1", "C14", "C12"]
        ]
    },
    "split": {
        "enabled": true,
        "serial": {
            "driver": "usart",
            "pin": "A9"
        },
        "handedness": {
            "pin": "C7"
        },
        "matrix_pins": {
