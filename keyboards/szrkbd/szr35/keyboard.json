                    ["B0", "B1", "C12", "D2", "B7"],
                    ["C5", "B12", "B3", "B4", "B5"],
                    ["C4", "A1", "C13", "NO_PIN", "NO_PIN"]
                ]
            }
        },
        "transport": {
            "sync": {
                "activity": true,
                "layer_state": true,
                "matrix_state": true
            },
            "watchdog": true,
            "watchdog_timeout": 3000
        },
        "bootmagic": {
            "matrix": [4, 4]
        }
    },
