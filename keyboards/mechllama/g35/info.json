{
    "keyboard_name": "MechLlama G35",
    "manufacturer": "kaylynb",
    "url": "https://github.com/kaylynb/MechLlama-G35",
    "maintainer": "kaylynb",
    "features": {
        "bootmagic": false,
        "extrakey": false,
        "mousekey": false,
        "nkro": true,
        "oled": true,
        "rgblight": true
    },
    "usb": {
        "vid": "0xCEEB",
        "pid": "0x0035"
    },
    "host": {
        "default": {
            "nkro": true
        }
    },
    "ws2812": {
        "pin": "F7"
    },
    "processor": "atmega32u4",
    "bootloader": "atmel-dfu",
    "layouts": {
        "LAYOUT": {
            "layout": [
                {"matrix": [0, 0], "x": 0, "y": 0.5},
                {"matrix": [0, 1], "x": 1, "y": 0.5},
